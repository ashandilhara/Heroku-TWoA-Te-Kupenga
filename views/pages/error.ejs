<!doctype html>
<html lang="en">
<head>
	<% include ../partials/header.ejs %>
    <style>
        #logoutLink{
            color: #fff;
        }

        #logoutLink:hover{
            color: #1DC7EA;
        }

    </style>
</head>
<body> 

<div class="wrapper">
    <% include ../partials/nav.ejs %>
    
    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">    
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">Notification</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                              <a id="logoutLink" class="btn btn-info btn-fill" href="/logout" >Logout</a>
                        </li>
                    </ul>       
                </div>
            </div>
        </nav>
        <div class="content">
            <div class="container-fluid">    
				<div style="margin-left: 15px; color: red;">Something went wrong, Please try again!</div>
				</br>
                <div class="row">
                    <div class="col-md-10" >
                        <%  if (hasOwnProperty("flash")){ %>
                        <% console.log('flash found'); %>
                        <% console.log(flash); %>
                            <% if(flash && flash.error) { %>
                                <div class="alert alert-danger" style="margin-left: 15px;">
                                    
                                    <%= flash.error %>
                                </div>
                            <% } %>    
                        <% } %>
                    </div>    
                </div>    
				<div class="form-group pull-left">
					<div class="col-sm-7">
						<a href="/" class="btn btn-success btn-fill" style="padding:5px 30px 5px 30px;">Go to Home</a>
					</div>
				</div>                        
            </div>    
        </div>
        
        <% include ../partials/footer.ejs %>
        
    </div>   
</div>

</body>

<% include ../partials/scripts.ejs %>

</html>